{
  "name": "smile-visa-project",
  "version": "1.0.0",
  "private": false,
  "author": "Heligate",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "create-network": "docker network create my_network",
    "run:dev": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose -f dev.docker-compose.yml build --parallel && docker-compose -f dev.docker-compose.yml up -d",
    "run:prod": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose -f docker-compose.prod.yml build --parallel && docker-compose -f docker-compose.prod.yml up -d",
    "stop-all-running-containers": "docker kill $(docker ps -q) && docker rm $(docker ps -a -q)",
    "free-space": "docker system prune -af --volumes"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.4",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^4.36.1",
    "@types/fs-extra": "^11.0.2",
    "antd": "^5.10.2",
    "axios": "^1.5.1",
    "fs-extra": "^11.1.1",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "log4js": "^6.9.1",
    "micro": "^10.0.1",
    "next": "^12.1.6",
    "next-i18next": "^13.1.4",
    "next-navigation": "^1.0.6",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-redux": "^8.1.3",
    "sass": "^1.53.0",
    "sequelize": "^6.33.0",
    "sqlite3": "^5.1.6",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.14",
    "autoprefixer": "^10.4.13",
    "daisyui": "^3.9.3",
    "eslint": "7.32.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,scss,css}": [
      "prettier --write '**/*.{js,jsx,ts,tsx,scss,css,json}'",
      "eslint --ext .js,.jsx,.ts,.tsx src --color",
      "git add ."
    ]
  }
}
